<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.UserMapper">
	<insert id="insert" parameterType="user">
		insert into tuser
		values(#{userId}, #{pass}, #{userName}, #{nickName}, #{email},
		#{phoneNum})
	</insert>
	
